<Project Sdk="Microsoft.NET.Sdk">
   <Import Project="..\..\..\common.props" />
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Description>Grpc模块：实现基于Grpc服务的信息的发送和接收</Description>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <RootNamespace>KissU.GrpcTransport</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.10.1" />
    <PackageReference Include="Grpc.Core" Version="2.24.0" />
    <PackageReference Include="Grpc.Reflection" Version="2.24.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\KissU.CPlatform\KissU.CPlatform.csproj" />
  </ItemGroup>
</Project>
