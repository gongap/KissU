@page
@model ApiDto
@*标题*@
<page-header title="编辑Api资源"></page-header>
@*表单*@
<util-card show-border="false">
    <util-form id="form" label-span="3" control-span="21" gutter="16">
        <util-textbox for="Name"></util-textbox>
        <util-textbox for="DisplayName"></util-textbox>
        <util-textbox for="Description"></util-textbox>
        <util-textbox for="ClaimTypes"></util-textbox>
        <util-switch for="Enabled"></util-switch>
        <util-form-item>
            <util-form-control offset="2">
                <util-button class="mr-sm" id="btnSubmit" validate-form="true" color="Primary" on-click="submit(form,btnSubmit)"></util-button>
                <util-button text="返回" on-click="back()"></util-button>
            </util-form-control>
        </util-form-item>
    </util-form>
</util-card>