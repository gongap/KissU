  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Mappings\SqlServer\ClientPropertyMap.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Util.Datas" Version="1.6.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\KissU.IdentityServer.Domain\KissU.IdentityServer.Domain.csproj" />
  </ItemGroup>

  <ItemGroup>
