@page
@model UserDto
@*标题*@
<page-header title="用户详情" [autoBreadcrumb]="false"></page-header>
@*表单*@
<util-card show-border="false">
    <util-description column="4" show-border="true">
        <util-description-item for="UserName" span="4"></util-description-item>
        <util-description-item for="PhoneNumber"></util-description-item>
        <util-description-item for="PhoneNumberConfirmed"></util-description-item>
        <util-description-item for="Email"></util-description-item>
        <util-description-item for="EmailConfirmed"></util-description-item>
        <util-description-item for="Enabled"></util-description-item>
        <util-description-item for="DisabledTime"></util-description-item>
        <util-description-item for="LockoutEnabled"></util-description-item>
        <util-description-item for="LockoutEnd"></util-description-item>
        <util-description-item for="TwoFactorEnabled"></util-description-item>
        <util-description-item for="RegisterIp"></util-description-item>
        <util-description-item for="LoginCount"></util-description-item>
        <util-description-item for="AccessFailedCount"></util-description-item>
        <util-description-item for="LastLoginTime"></util-description-item>
        <util-description-item for="LastLoginIp"></util-description-item>
        <util-description-item for="CurrentLoginTime"></util-description-item>
        <util-description-item for="CurrentLoginIp"></util-description-item>
        <util-description-item for="CreationTime" date-format="yyyy-MM-dd HH:mm:ss"></util-description-item>
        <util-description-item for="Remark" span="3"></util-description-item>
    </util-description>
</util-card>