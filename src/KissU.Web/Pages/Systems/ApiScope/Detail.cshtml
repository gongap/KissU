@page
@model ApiScopeDto
@*标题*@
<page-header title="Api许可范围详情"></page-header>
@*表单*@
<util-card show-border="false" actions="[actionBack]">
    <util-description title="详细信息" column="3" show-border="true" >
        <util-description-item for="ApiId"></util-description-item>
        <util-description-item for="Name"></util-description-item>
        <util-description-item for="DisplayName"></util-description-item>
        <util-description-item for="Description"></util-description-item>
        <util-description-item for="ClaimTypes"></util-description-item>
        <util-description-item for="Required"></util-description-item>
        <util-description-item for="Emphasize"></util-description-item>
        <util-description-item for="ShowInDiscoveryDocument"></util-description-item>
        <util-description-item for="CreationTime"></util-description-item>
    </util-description>
    <util-template id="actionBack">
        <util-button text="返回" on-click="back()"></util-button>
    </util-template>
</util-card>