@page
@model RoleDto
@*标题*@
<page-header title="编辑角色"></page-header>
@*表单*@
<util-card show-border="false">
    <util-form id="form" label-span="3" control-span="18" gutter="16">
        <util-textbox for="Code"></util-textbox>
        <util-textbox for="Name"></util-textbox>
        <util-textbox for="NormalizedName"></util-textbox>
        <util-textbox for="Type"></util-textbox>
        <util-switch for="IsAdmin"></util-switch>
        <util-textbox for="Comment"></util-textbox>
        <util-textbox for="PinYin"></util-textbox>
        <util-textbox for="Sign"></util-textbox>
        <util-form-item>
            <util-form-control offset="2">
                <util-button class="mr-sm" id="btnSubmit" validate-form="true" color="Primary" on-click="submit(form,btnSubmit)"></util-button>
                <util-button text="返回" on-click="back()"></util-button>
            </util-form-control>
        </util-form-item>
    </util-form>
</util-card>