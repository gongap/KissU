@page
@model LinkDto
@*标题*@
<page-header title="链接详情"></page-header>
@*表单*@
<util-card show-border="false" actions="[actionBack]">
    <util-description title="详细信息" column="3" show-border="true" >
        <util-description-item for="Code"></util-description-item>
        <util-description-item for="Title"></util-description-item>
        <util-description-item for="Address"></util-description-item>
        <util-description-item for="Image"></util-description-item>
        <util-description-item for="Comment"></util-description-item>
        <util-description-item for="Enabled"></util-description-item>
        <util-description-item for="Status"></util-description-item>
        <util-description-item for="CreationTime"></util-description-item>
    </util-description>
    <util-template id="actionBack">
        <util-button text="返回" on-click="back()"></util-button>
    </util-template>
</util-card>