<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="../../../../common.props" />
  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <Description>通过AutoMapper组件实现Dto对象和实体对象之间的相互转换</Description>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\..\..\..\output\core\netcoreapp2.2</OutputPath>
    <DocumentationFile>..\..\..\..\output\core\netcoreapp2.2\KissU.Core.AutoMapper.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
      <OutputPath>..\..\..\..\output\core\netcoreapp2.2</OutputPath>
    <DocumentationFile>..\..\..\..\output\core\netcoreapp2.2\KissU.Core.AutoMapper.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper.Attributes" Version="6.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\KissU.Core.CPlatform\KissU.Core.CPlatform.csproj" />
  </ItemGroup>
</Project>
