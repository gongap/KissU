<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="../../../../common.props" />
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Description>KissU应用框架的支付操作类库。</Description>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\KissU.Util.Logs\KissU.Util.Logs.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\PayResource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>PayResource.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\PayResource.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>PayResource.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
</Project>