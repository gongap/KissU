    <PackageReference Include="KissU.DotNetty" Version="1.0.0-preview-20200808-01" />
    <PackageReference Include="KissU.DotNetty.WebSocket" Version="1.0.0-preview-20200808-01" />
    <PackageReference Include="KissU.Kestrel.Http" Version="1.0.0-preview-20200808-01" />
    <PackageReference Include="KissU.Kestrel.Nlog" Version="1.0.0-preview-20200808-01" />
    <PackageReference Include="KissU.Kestrel.Swagger" Version="1.0.0-preview-20200808-01" />
    <PackageReference Include="KissU.Kestrel.IdentityServer" Version="1.0.0-preview-20200808-01" />
    <PackageReference Include="KissU.DotNetty.Http" Version="1.0.0-preview-20200808-01" />
    <PackageReference Include="KissU.DotNetty.Mqtt" Version="1.0.0-preview-20200808-01" />
