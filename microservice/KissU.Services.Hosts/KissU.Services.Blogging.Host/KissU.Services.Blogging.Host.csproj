  <ItemGroup>
    <ProjectReference Include="..\..\KissU.Modules\blogging\src\KissU.Modules.Blogging.Service\KissU.Modules.Blogging.Service.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="apollo.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="appsettings.Development.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="appsettings.json">
